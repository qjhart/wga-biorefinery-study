@ARTICLE{Fargione2008,
  author =	 {Fargione, Joseph and Hill, Jason and Tilman, David
                  and Polasky, Stephen and Hawthorne, Peter},
  title =	 {Land Clearing and the Biofuel Carbon Debt},
  journal =	 {Science},
  year =	 2008,
  volume =	 319,
  pages =	 {1235-1238},
  number =	 5867,
  abstract =	 {Increasing energy use, climate change, and carbon
                  dioxide (CO2) emissions from fossil fuels make
                  switching to low-carbon fuels a high priority.
                  Biofuels are a potential low-carbon energy source,
                  but whether biofuels offer carbon savings depends on
                  how they are produced. Converting rainforests,
                  peatlands, savannas, or grasslands to produce food
                  cropbased biofuels in Brazil, Southeast Asia, and
                  the United States creates a "biofuel carbon debt" by
                  releasing 17 to 420 times more CO2 than the annual
                  greenhouse gas (GHG) reductions that these biofuels
                  would provide by displacing fossil fuels. In
                  contrast, biofuels made from waste biomass or from
                  biomass grown on degraded and abandoned agricultural
                  lands planted with perennials incur little or no
                  carbon debt and can offer immediate and sustained
                  GHG advantages.}
}

